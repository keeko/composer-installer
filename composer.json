{
	"name" : "keeko/composer-installer",
	"description" : "Keeko component installer",
	"type" : "composer-plugin",
	"authors" : [{
			"name" : "gossi",
			"homepage" : "http://gos.si"
		}
	],
	"license" : "MIT",
	"require" : {
		"php" : ">=5.4",
		"composer-plugin-api" : "1.0.0"
	},
	"require-dev" : {
		"composer/composer" : "master-dev"
	},
	"autoload" : {
		"psr-0" : {
			"keeko\\composer" : "src"
		}
	},
	"extra" : {
		"class" : "\\keeko\\composer\\KeekoComposerPlugin"
	}
}